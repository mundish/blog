{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "publish": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.3.1",
    "@mdx-js/react": "^1.3.1",
    "gatsby": "2.13.25",
    "gatsby-plugin-mdx": "^1.0.30",
    "gatsby-plugin-postcss": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-source-filesystem": "^2.1.11",
    "gatsby-transformer-remark": "^2.6.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@weahead/prettier-config": "0.1.3",
    "gh-pages": "^2.1.1",
    "prettier": "1.18.2",
    "tailwindcss": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
